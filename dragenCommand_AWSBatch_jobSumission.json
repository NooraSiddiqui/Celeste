{
    "jobName": "dragen-WGS-full-pipeline-analysis",
    "jobQueue": "dragen-queue-prod",
    "jobDefinition": "dragen-prod",
    "containerOverrides": {
        "command": [
            "--force",
            "--board-count=1",
            "--ref-dir",
            "s3://${OperationsDataBucket}/bwa_references/h/grch38/grch38_hashtable/",
            "--fastq-list",
            "s3://PATH",
            "--fastq-list-sample-id",
            "SAMPLE_ID",
            "--output-directory",
            "s3://PAHT",
            "--output-file-prefix",
            "OUTPUT_PREFIX",
            "--enable-variant-caller",
            "true",
            "--enable-duplicate-marking",
            "true",
            "--enable-map-align",
            "true",
            "--enable-map-align-output",
            "true",
            "--output-format=CRAM",
            "--vc-hard-filter",
            "'DRAGENHardQUAL:all:QUAL<5.0;LowDepth:all:DP<=1'",
            "--vc-frd-max-effective-depth=40",
            "--qc-cross-cont-vcf",
            "s3://${OperationsDataBucket}/AoU-resources/hg38-resources/SNP_NCBI_GRCh38.vcf",
            "--qc-coverage-region-1",
            "s3://${OperationsDataBucket}/AoU-resources/hg38-resources/wgs_coverage_regions.hg38_minus_N.interval_list.bed",
            "--qc-coverage-reports-1",
            "cov_report",
            "--qc-coverage-region-2",
            "s3://${OperationsDataBucket}/AoU-resources/hg38-resources/acmg59_allofus_19dec2019.GRC38.wGenes.bed",
            "--qc-coverage-reports-2",
            "cov_report",
            "--qc-coverage-region-3",
            "s3://${OperationsDataBucket}/AoU-resources/hg38-resources/PGx_singleSite_GRCh38_ActualTableToSendToFDA_21jan2020.bed",
            "--qc-coverage-reports-3",
            "cov_report",
            "--lic-server",
            "DRAGEN_LICENSE"
        ]
    },
    "retryStrategy": {"attempts": 2}
}
